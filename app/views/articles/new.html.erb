<h2><strong>Create new article</strong></h2>
<br></br>
<div class = "col-md-4 col-md-offset-4">
  <div class = "NewArticle">
<%= bootstrap_form_for @article do |f| %>
  <%= f.text_field :title, placeholder: 'Title' %>
  <%= f.text_area :content, placeholder: 'Content' %>
  <%= f.url_field :image, placeholder: 'Image Url' %>
  <%= f.number_field :muni_id, placeholder: 'Muni Line' %>
  <br>
  <div class = "CreateButton">
  <%= f.submit "Create new article" %>
<% end %>
</div>
</div>
</div>
